{
  "name": "ticaje/hexagonal",
  "description": "Library that defines hexagonal-design based policies",
  "authors": [
    {
      "name": "Max Demian",
      "email": "ticaje@filetea.me",
      "homepage": "https://www.github.com/ticaje"
    }
  ],
  "version": "1.0.7",
  "type": "library",
  "keywords": [
    "Domain Driven Design",
    "Port and Adapters",
    "Hexagonal",
    "Design by Contract"
  ],
  "license": [
    "OSL-3.0",
    "AFL-3.0"
  ],
  "require": {
